#include <iostream>
using namespace std;

int main()
{
    string str;
    cin>>str;
    int n=str.length();
    string ans;
    string value[]={"2","22","222",
                    "3","33","333",
                    "4","44","444",
                    "5","55","555",
                    "6","66","666",
                    "7","77","777","7777",
                    "8","88","888",
                    "9","99","999","9999"};
    
    for(int i=0;i<n;i++)
    {
        ans=ans+value[str[i]-'A'];
        
    }
    cout<<ans<<endl;
    return 0;
}
